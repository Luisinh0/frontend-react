# .env.example
# Copia este archivo a .env y ajusta los valores según tu configuración

# URL base de la API REST del proceso ETL
VITE_API_BASE_URL=http://localhost:3000/api

# Modo de desarrollo (usar datos mockeados)
VITE_USE_MOCK_DATA=true

# Intervalo de auto-refresh en milisegundos (0 = deshabilitado)
VITE_REFRESH_INTERVAL=30000
